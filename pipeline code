//simple hello world pipeline

pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    }
}




//main pipelines
pipeline {
    agent any

    stages {
        stage('compile') {
            steps {
                echo 'compile'
            }
            
            stage('test') {
            steps {
                echo 'test'
            }
            
            stage('deploy') {
            steps {
                echo 'deploy'
            }
        }
    }
}
